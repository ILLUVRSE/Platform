{
  "extends": "../../packages/config/tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@food/*": [
        "./src/food/*"
      ],
      "@gridstock/*": [
        "./src/gridstock/*"
      ],
      "@studio/*": [
        "./src/app/studio/*"
      ],
      "@news/*": [
        "./src/news/*"
      ]
    },
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
